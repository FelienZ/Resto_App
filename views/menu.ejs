    <main class="w-full screen flex justify-center bg-gray-200">
        <div class="interfaces flex flex-col items-center justify-center py-10 md:w-[70%] min-h-screen">
        <h2 class="font-bold underline text-2xl text-gray-800">List Menu: </h2>
        <div class="animation flex gap-3 mt-5">
            <p class="text-gray-800 font-bold">Loading</p>
            <span class="loading loading-dots bg-gray-800 loading-xl"></span>
        </div>
        <div class="information hidden">
          <% if (menu.length < 1) { %>
             <p class="flex w-full justify-center to-gray-800 font-bold"> Menu Kosong! </p>
        <% } %>
            <div class="grid md:grid-cols-3 gap-5 mb-5">
                <% for( let index = 0; index < menu.length; index++ ) { %>
                    <div class="card bg-white px-10 py-3 rounded-md grid gap-3 border-2 border-gray-500 text-black text-center" data-aos="zoom-in">
                    <img src="<%= menu[index].pic %>" alt="foto" class="rounded-md">
                    <p class="font-bold text-gray-800"><%= nama = menu[index].menuName %></p>
                    <p>Harga: <%= Harga = menu[index].price %></p>
                    <button class="w-full py-1 rounded-md bg-gray-800 text-white">Pesan</button>
                    </div>
                <% } %>
            </div>
        </div>  
    </div>          
    </main>
